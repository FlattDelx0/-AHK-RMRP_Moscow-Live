FormatPrice(Price) {
    StringReplace, Price, Price, %A_Space%,, All
    StringReplace, Price, Price, %A_Tab%,, All
    Price := RegExReplace(Price, "[^\d]", "")
    if (Price = "") {
        Return "0"
    }
    Loop
    {
        StringLen, len, Price
        if (len <= 3)
            Break
        part := SubStr(Price, len-2, 3)
        formatted := "." part formatted
        Price := SubStr(Price, 1, len-3)
    }
    formatted := Price . formatted
    if (SubStr(formatted, 1, 1) = ".")
        formatted := SubStr(formatted, 2)

    Return formatted
}

