ScriptVersion := "1.0.0"

CheckForUpdates() {
    global ScriptVersion
    UpdateUrl := "https://raw.githubusercontent.com/ВАШ_ЛОГИН/ВАШ_РЕПОЗИТОРИЙ/main/version.txt"
    FileUrl := "https://raw.githubusercontent.com/ВАШ_ЛОГИН/ВАШ_РЕПОЗИТОРИЙ/main/ВАШ_СКРИПТ.ahk"

    ; Загружаем версию с GitHub
    UrlDownloadToFile, %UpdateUrl%, temp_version.txt
    FileRead, LatestVersion, temp_version.txt
    FileDelete, temp_version.txt

    ; Убираем пробелы и переносы строк
    StringTrimRight, LatestVersion, LatestVersion, 1

    ; Проверяем версии
    if (LatestVersion != "" && ScriptVersion != LatestVersion) {
        MsgBox, Обнаружено обновление! Доступна версия %LatestVersion% (у вас %ScriptVersion%). Загрузить?
        IfMsgBox, Yes
        {
            UrlDownloadToFile, %FileUrl%, UpdatedScript.ahk
            MsgBox, Обновлённый скрипт загружен как UpdatedScript.ahk. Перезапустите скрипт вручную.
            ExitApp ; Завершаем текущий скрипт
        }
    } else {
        MsgBox, У вас актуальная версия скрипта (%ScriptVersion%).
    }
}


